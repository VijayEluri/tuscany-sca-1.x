  <Table name="CUSTOMER">
    <Column name="ID" primaryKey="true"/>
    <Column name="LASTNAME"/>
    <Column name="ADDRESS"/> 
    <create>insert into customer values (:ID, :LASTNAME, :ADDRESS)</create>
    <update>update customer set lastname = :LASTNAME, address = :ADDRESS where ID = :ID</update>
    <delete>delete from customer where ID = :ID</delete>
  </Table>
</Config>