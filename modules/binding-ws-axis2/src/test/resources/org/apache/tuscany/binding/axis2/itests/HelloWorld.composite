<composite xmlns="http://www.osoa.org/xmlns/sca/1.0"
           xmlns:wsdli="http://www.w3.org/2006/01/wsdl-instance"

           name="HelloWorld">

    <component name="HelloWorldComponent">
		<implementation.java class="org.apache.tuscany.binidng.axis2.itests.HelloWorldComponent"/>
        <reference name="ref" target="HelloWorldWS"></reference>
    </component>

    <reference name="HelloWorldWS">
        <interface.wsdl interface="http://helloworld#wsdl.interface(HelloWorld)" 
            wsdli:wsdlLocation="http://helloworld org/apache/tuscany/binding/axis2/itests/helloworld.wsdl" />
        <binding.ws/>
    </reference>

</composite>
